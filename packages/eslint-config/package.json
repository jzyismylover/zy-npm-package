{
  "name": "@bestzy/eslint-config",
  "version": "1.0.0",
  "description": "zy的ESLint配置规则",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./typescript": {
      "types": "./dist/typescript.d.ts",
      "import": "./dist/typescript.js"
    },
    "./react": {
      "types": "./dist/react.d.ts",
      "import": "./dist/react.js"
    },
    "./vue": {
      "types": "./dist/vue.d.ts",
      "import": "./dist/vue.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "typecheck": "tsc --noEmit",
    "test": "echo \"测试待添加\"",
    "lint": "echo \"已是lint规则\""
  },
  "keywords": [
    "eslint",
    "eslint-config",
    "typescript",
    "javascript",
    "zy"
  ],
  "author": "zy",
  "license": "MIT",
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-vue": "^9.19.0"
  },
  "peerDependencies": {
    "eslint": "^8.0.0",
    "typescript": ">=4.0.0"
  },
  "devDependencies": {
    "typescript": "^5.3.0",
    "rimraf": "^5.0.5"
  }
}
